<h1> Here is users in backend </h1>

<table>
  <thead>
    <tr>
      <th> Users exists </th>
    </tr>
  </thead>
  <% @users.each do |user| %>
    <% if logged_in? && current_user.admin? %>
      <tbody>
            <td> <%= user.username%> </td>
            <td> <%= user.email%> </td>
            <td>
              <%= link_to "Show", user_path(user)%>
              <%= link_to "Edit", edit_user_path(user)%>
            </td>
            <td> <%= link_to "Destroy", user, method: :delete, data: {confirm: "Are you sure?"} %> </td>
      </tbody>
    <% end %>
  <% end%>
</table>